import java.util.Scanner;
public class largestnumber{
    public static boolean containdigit(int n,int d){
        String numstr=Integer.toString(n);
        char dig=(char)('0'+d);
        return numstr.indexOf(dig)!=-1;
    }
    
    public static int findlarge(int n,int d){
        for(int i=n-1;i>=0;i--){
            if(!containdigit(i,d))
            return i;
        }
        return -1;
    }
    public static void main(String[]args){
        Scanner scan=new Scanner(System.in);
        
        int n=scan.nextInt();
        int d=scan.nextInt();
        
        System.out.println(findlarge(n,d));
    }
}
